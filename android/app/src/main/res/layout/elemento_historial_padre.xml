<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/elementoHistorialPadre"
    android:visibility="visible">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/elementoHistorialReducido"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="4dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/puntuacionRespiratoriaHistorial"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="64dp"
            android:text="88/100"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="@+id/iconoPuntRespiratoria"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/iconoPuntRespiratoria"
            app:layout_constraintTop_toTopOf="@+id/iconoPuntRespiratoria" />

        <TextView
            android:id="@+id/tiempoSuenioHistorial"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="32dp"
            android:text="8h 25min"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="@+id/iconoTiempoSuenio"
            app:layout_constraintEnd_toStartOf="@+id/iconoPuntRespiratoria"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/iconoTiempoSuenio"
            app:layout_constraintTop_toTopOf="@+id/iconoTiempoSuenio" />

        <ImageView
            android:id="@+id/iconoTiempoSuenio"
            android:layout_width="21dp"
            android:layout_height="20dp"
            android:layout_marginStart="32dp"
            android:layout_marginEnd="8dp"
            android:contentDescription="fotografía"
            android:src="@drawable/icons8_health_64"
            app:layout_constraintBottom_toBottomOf="@+id/fechaElementoHistorial"
            app:layout_constraintEnd_toStartOf="@+id/tiempoSuenioHistorial"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/fechaElementoHistorial"
            app:layout_constraintTop_toTopOf="@+id/fechaElementoHistorial" />

        <ImageView
            android:id="@+id/iconoPuntRespiratoria"
            android:layout_width="21dp"
            android:layout_height="20dp"
            android:layout_marginEnd="8dp"
            android:contentDescription="fotografía"
            android:src="@drawable/icons8_breath_100"
            app:layout_constraintBottom_toBottomOf="@+id/iconoTiempoSuenio"
            app:layout_constraintEnd_toStartOf="@+id/puntuacionRespiratoriaHistorial"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/tiempoSuenioHistorial"
            app:layout_constraintTop_toTopOf="@+id/iconoTiempoSuenio" />

        <TextView
            android:id="@+id/fechaElementoHistorial"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="64dp"
            android:layout_marginTop="8dp"
            android:text="05/11/2023"
            android:textSize="12sp"
            app:layout_constraintEnd_toStartOf="@+id/iconoTiempoSuenio"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/elementoHistorialCompleto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="4dp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/puntuacionRespiratoriaHistorialCompleto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="88/100"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/iconoPuntRespiratoriaCompleto"
            app:layout_constraintStart_toEndOf="@+id/textView22"
            app:layout_constraintTop_toTopOf="@+id/iconoPuntRespiratoriaCompleto" />

        <TextView
            android:id="@+id/temperaturaMediaHistorial"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="24C"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/iconoTempMedia"
            app:layout_constraintStart_toEndOf="@+id/textView19"
            app:layout_constraintTop_toTopOf="@+id/iconoTempMedia" />

        <TextView
            android:id="@+id/tiempoSuenioHistorialCompleto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="8h 25min"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/iconoTiempoSuenioCompleto"
            app:layout_constraintStart_toEndOf="@+id/textView21"
            app:layout_constraintTop_toTopOf="@+id/iconoTiempoSuenioCompleto" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="Tiempo de sueño:"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/iconoTiempoSuenioCompleto"
            app:layout_constraintStart_toEndOf="@+id/iconoTiempoSuenioCompleto"
            app:layout_constraintTop_toTopOf="@+id/iconoTiempoSuenioCompleto" />

        <TextView
            android:id="@+id/textView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="Puntuacion respiratoria:"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/iconoPuntRespiratoriaCompleto"
            app:layout_constraintStart_toEndOf="@+id/iconoTiempoSuenioCompleto"
            app:layout_constraintTop_toTopOf="@+id/iconoPuntRespiratoriaCompleto" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="Temperatura media:"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/iconoTempMedia"
            app:layout_constraintStart_toEndOf="@+id/iconoTiempoSuenioCompleto"
            app:layout_constraintTop_toTopOf="@+id/iconoTempMedia" />

        <ImageView
            android:id="@+id/iconoTiempoSuenioCompleto"
            android:layout_width="21dp"
            android:layout_height="20dp"
            android:layout_marginTop="4dp"
            android:contentDescription="fotografía"
            android:src="@drawable/icons8_health_64"
            app:layout_constraintStart_toStartOf="@+id/fechaElementoHistorialCompleto"
            app:layout_constraintTop_toBottomOf="@+id/fechaElementoHistorialCompleto" />

        <ImageView
            android:id="@+id/iconoTempMedia"
            android:layout_width="21dp"
            android:layout_height="20dp"
            android:layout_marginTop="4dp"
            android:contentDescription="fotografía"
            android:src="@drawable/icons8_temperature_96"
            app:layout_constraintStart_toStartOf="@+id/iconoTiempoSuenioCompleto"
            app:layout_constraintTop_toBottomOf="@+id/iconoTiempoSuenioCompleto" />

        <ImageView
            android:id="@+id/iconoPuntRespiratoriaCompleto"
            android:layout_width="21dp"
            android:layout_height="20dp"
            android:layout_marginTop="4dp"
            android:contentDescription="fotografía"
            android:src="@drawable/icons8_breath_100"
            app:layout_constraintStart_toStartOf="@+id/iconoTiempoSuenioCompleto"
            app:layout_constraintTop_toBottomOf="@+id/iconoTempMedia" />

        <TextView
            android:id="@+id/fechaElementoHistorialCompleto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="05/11/2023"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/puntuacionTextoHistorial"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:text="aqui va bueno"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
